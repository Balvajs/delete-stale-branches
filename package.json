{
  "name": "delete-stale-branches",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "type-check": "tsc",
    "format": "prettier . --write -u",
    "format-check": "prettier . --check -u",
    "lint": "eslint src/**/*.ts",
    "package": "tsup src/main.ts --clean --sourcemap",
    "test": "jest --passWithNoTests",
    "generate": "graphql-codegen-esm --config codegen.ts",
    "all": "pnpm type-check && pnpm format && pnpm lint && pnpm package && pnpm test",
    "semantic-release": "semantic-release",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Balvajs/delete-stale-branches.git"
  },
  "keywords": [
    "actions"
  ],
  "engines": {
    "node": "=18"
  },
  "packageManager": "pnpm@8.6.12",
  "type": "module",
  "author": "Balvajs",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,json,yml,md,mdx}": "prettier --write"
  },
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@graphql-codegen/add": "5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@octokit/core": "5.0.0",
    "@octokit/graphql": "7.0.1",
    "@octokit/graphql-schema": "14.27.2",
    "@octokit/plugin-paginate-graphql": "4.0.0",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "11.0.4",
    "@swc/core": "1.3.78",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.6",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "chalk": "5.3.0",
    "dayjs": "1.11.9",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-github": "4.9.2",
    "eslint-plugin-i18n-text": "1.0.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "graphql": "16.8.0",
    "husky": "8.0.3",
    "jest": "29.6.3",
    "lint-staged": "14.0.1",
    "node-fetch": "3.3.2",
    "prettier": "3.0.2",
    "semantic-release": "21.0.9",
    "semantic-release-major-tag": "0.3.2",
    "ts-node": "10.9.1",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  }
}
